{
  "name": "decyphr",
  "description": "Perform actual translations on your site",
  "version": "0.9.0",
  "author": "Aaron Sinnott @aaronsnig501",
  "bin": {
    "decyphr": "./bin/run"
  },
  "bugs": "https://github.com/decyphr-net/i18nr/issues",
  "dependencies": {
    "@oclif/command": "^1.6.1",
    "@oclif/config": "^1.15.1",
    "@oclif/plugin-help": "^3.1.0",
    "axios": "^0.21.1",
    "json-to-pretty-yaml": "^1.2.2",
    "laravelphp": "^2.0.3",
    "tslib": "^1.13.0",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/test": "^1.2.6",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.17.26",
    "@types/yamljs": "^0.2.31",
    "chai": "^4.2.0",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "globby": "^10.0.2",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/decyphr-net/i18nr",
  "keywords": [
    "oclif",
    "translator",
    "translations",
    "translate",
    "internationalization",
    "i18n"
  ],
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "decyphr",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "decyphr-net/i18nr",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
